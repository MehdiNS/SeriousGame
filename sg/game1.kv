#:kivy 1.8.0

<Object>:
    size: 100, 100
	on_touch_down: root.on_touch_move


<Game1>:
    
    Object:
        x: root.x + 50
        center_y: root.center_y
        canvas:
	        Rectangle:
	            pos: self.pos
	            size: self.size
	            source: '../image/Dessins/Habitations/Habitat1.tif'
		text: '../sound/CEstUne/CEstUneDena.wav'
        
	Object:
        x: root.x
        center_y: root.center_y +100
        canvas:
	        Rectangle:
	            pos: self.pos
	            size: self.size
	            source: '../image/Dessins/Habitations/Habitat2.tif'
		text: '../sound/CEstUne/CEstUneBinsa.wav'
        
        
        