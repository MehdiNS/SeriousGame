#:kivy 1.8.0

# Initialization of the different classes

#Class for Houses	
<CategoryHouse>:
	category: "house"   
	name: "Cat1"
	canvas:
	    Rectangle:
	        pos: self.pos
	        size: self.size
	        source: '../image/Dessins/Habitations/Habitat1.tif'

#Class for Vehicles
<CategoryVehicle>:
	size: 200,200
	category: "vehicle"
	name: "Cat2"
	canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source: '../image/Dessins/Vehicules/Vehicule3.tif'

#Class for Characters	
<CategoryCharacter>:
    size: 200, 200
    category: "character"
    name: "Cat3"
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source: '../image/Dessins/Personnages/Perso2.tif'
    		    		
#Class for all different objects
<Object>:
	pos_base: [0,0]



# List of objects used in the class Game2
# The following list match the organization of objects
# A  B  C
# 1  1  1
# 2  2  2
# 3  3  3
# 4  4  4
# 5  5  5
# 6  6  6
